bundle agent secrets {
  vars:
    "shared" data => parsejson(execresult(
        "/usr/bin/gpg --decrypt $(sys.inputsdir)/secrets.json.gpg"
      , "noshell"));

    "test" string => execresult(
        "/usr/bin/gpg --decrypt $(sys.inputsdir)/secrets.json.gpg"
      , "noshell");

    "keys" slist => getindices("shared");

  reports:
    "The shared secrets are $(shared[mail_gmail_password])";
    "The sahred secrets keys are ... $(keys) ";
    "TEST TEST $(test)";
}
